<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="other_settings">
        <fieldset name="eshopper">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="label" xsi:type="string" translate="true">E-shopper Settings</item>
                    <item name="collapsible" xsi:type="boolean">true</item>
                    <item name="level" xsi:type="number">1</item>
                </item>
            </argument>
            <fieldset name="header">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="label" xsi:type="string" translate="true">Head</item>
                        <item name="sortOrder" xsi:type="number">10</item>
                        <item name="level" xsi:type="number">2</item>
                    </item>
                </argument>
                <field name="phone_number">
                    <argument name="data" xsi:type="array">
                        <item name="config" xsi:type="array">
                            <item name="label" xsi:type="string" translate="true">Phone Number </item>
                            <item name="dataType" xsi:type="string">text</item>
                            <item name="formElement" xsi:type="string">input</item>
                            <item name="dataScope" xsi:type="string">phone_number</item>
                            <item name="validation" xsi:type="array">
                                <item name="validate-no-html-tags" xsi:type="boolean">true</item>
                            </item>
                        </item>
                    </argument>
                </field>
                <field name="email_address">
                    <argument name="data" xsi:type="array">
                        <item name="config" xsi:type="array">
                            <item name="label" xsi:type="string" translate="true">Email Address </item>
                            <item name="dataType" xsi:type="string">text</item>
                            <item name="formElement" xsi:type="string">input</item>
                            <item name="dataScope" xsi:type="string">email_address</item>
                            <item name="validation" xsi:type="array">
                                <item name="validate-no-html-tags" xsi:type="boolean">true</item>
                            </item>
                        </item>
                    </argument>
                </field>
            </fieldset>

        <fieldset name="social_media">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="label" xsi:type="string" translate="true">Social Media</item>
                    <item name="sortOrder" xsi:type="number">20</item>
                    <item name="level" xsi:type="number">2</item>
                </item>
            </argument>
            <field name="social_fb">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="label" xsi:type="string" translate="true">Facebook Url </item>
                        <item name="dataType" xsi:type="string">text</item>
                        <item name="formElement" xsi:type="string">input</item>
                        <item name="dataScope" xsi:type="string">social_fb</item>
                        <item name="validation" xsi:type="array">
                            <item name="validate-no-html-tags" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
            </field>
            <field name="social_tw">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="label" xsi:type="string" translate="true">Twitter Url </item>
                        <item name="dataType" xsi:type="string">text</item>
                        <item name="formElement" xsi:type="string">input</item>
                        <item name="dataScope" xsi:type="string">social_tw</item>
                        <item name="validation" xsi:type="array">
                            <item name="validate-no-html-tags" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
            </field>
            <field name="social_in">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="label" xsi:type="string" translate="true">IN Url </item>
                        <item name="dataType" xsi:type="string">text</item>
                        <item name="formElement" xsi:type="string">input</item>
                        <item name="dataScope" xsi:type="string">social_in</item>
                        <item name="validation" xsi:type="array">
                            <item name="validate-no-html-tags" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
            </field>
            <field name="social_dr">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="label" xsi:type="string" translate="true">Dribbble Url </item>
                        <item name="dataType" xsi:type="string">text</item>
                        <item name="formElement" xsi:type="string">input</item>
                        <item name="dataScope" xsi:type="string">social_dr</item>
                        <item name="validation" xsi:type="array">
                            <item name="validate-no-html-tags" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
            </field>
            <field name="social_gp">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="label" xsi:type="string" translate="true">Google Plus Url </item>
                        <item name="dataType" xsi:type="string">text</item>
                        <item name="formElement" xsi:type="string">input</item>
                        <item name="dataScope" xsi:type="string">social_gp</item>
                        <item name="validation" xsi:type="array">
                            <item name="validate-no-html-tags" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
            </field>
        </fieldset>
        </fieldset>
    </fieldset>
</form>
